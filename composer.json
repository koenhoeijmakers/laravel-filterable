{
    "name": "koenhoeijmakers/laravel-filterable",
    "description": "Explicit request filtering.",
    "require": {
        "php": ">=8.1",
        "laravel/framework": "^10.0"
    },
    "require-dev": {
        "orchestra/testbench": "^8.0",
        "phpunit/phpunit": "^10.0"
    },
    "type": "library",
    "license": "MIT",
    "minimum-stability": "stable",
    "prefer-stable": true,
    "authors": [
        {
            "name": "Koen Hoeijmakers",
            "email": "koen@hoeijmakers.me"
        }
    ],
    "autoload": {
        "psr-4": {
            "KoenHoeijmakers\\LaravelFilterable\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "KoenHoeijmakers\\LaravelFilterable\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "KoenHoeijmakers\\LaravelFilterable\\FilterableServiceProvider"
            ]
        }
    }
}
